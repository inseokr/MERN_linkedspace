<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>                                                                                              
<link rel="stylesheet" href="/stylesheets/step_wizard.css">
	<!-- totalSteps/curr_step/action_url/action_method should be provided -->
	<div class="stepwizard offset-md-3" style="margin-top: 20px">
		<div class="stepwizard-row setup-panel">
			<%for (var step = 1; step <= totalSteps; step++){%>
				<div class="stepwizard-step">
					<a class="btn <%if(curr_step==step){%>
										btn-primary
					               <%}else{%>
					                 	btn-outline-secondary
					               <%}%>
					          btn-circle" href='<%="/listing/landlord/"+listing_info.listing_id+"/step"+step%>' type="button"><%=step%></a>
				</div>
		  <%}%>
		</div>
	</div> <!-- stepwizard -->