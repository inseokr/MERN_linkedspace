<script>
// Initialize and add the map
function initMap() {
  var mapUrl = document.getElementById('map_url');
  var urlString = mapUrl.value.trim();

  $.getJSON(urlString, function(data)
  {
        var p = data.results[0].geometry.location;
        // The map, centered at Uluru
        var map = new google.maps.Map(
          document.getElementById('map'), {zoom: 15, center: p});
        var marker = new google.maps.Marker({position: p, map: map});
  });
}

</script>
<script async defer
src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDtSupWCIi2l-ZFk5jzVpI2gucIbFP_oCY&callback=initMap">
